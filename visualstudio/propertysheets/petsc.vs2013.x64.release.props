<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets">
  </ImportGroup>
  <PropertyGroup Label="UserMacros">
    <PETSC_ROOT>Z:\code\repos\petsc-3.4.3-vs2013</PETSC_ROOT>
  </PropertyGroup>
  <PropertyGroup>
    <LibraryPath>$(PETSC_ROOT)\win64\lib;$(LibraryPath)</LibraryPath>
    <ExecutablePath>$(ExecutablePath)</ExecutablePath>
    <IncludePath>$(PETSC_ROOT)\win64\include;$(PETSC_ROOT)\include;$(IncludePath)</IncludePath>
    <_PropertySheetDisplayName>petsc.vs2013.x64.release</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <PreprocessorDefinitions>%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
    <Link>
      <AdditionalDependencies>libf2cblas.lib;libf2clapack.lib;libpetsc.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="PETSC_ROOT">
      <Value>$(PETSC_ROOT)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>